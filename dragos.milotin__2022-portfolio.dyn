      "Code": "# Copyright(c) 2020, Dragos Milotin\r\n# @milotindragos, http://archi.tools\r\n\r\n# START: DEFINE IMPORTS\r\nimport sys\r\nsys.path.append(r'C:\\Program Files (x86)\\IronPython 2.7\\Lib')\r\n\r\nimport urllib2\r\nimport webbrowser\r\n\r\nimport clr\r\nclr.AddReference(\"System.Drawing\")\r\nfrom System.Drawing import Point, Size, Font, FontStyle\r\nclr.AddReference(\"System.Windows.Forms\")\r\nfrom System.Windows.Forms import Application, Form, PictureBox, PictureBoxSizeMode, Label, Button, DialogResult\r\n# END: DEFINE IMPORTS\r\n\r\n# START: DEFINE INPUTS\r\nboolean = IN[0]\r\nmessage = IN[1]\r\nimageURL = IN[2]\r\nportfolioItemURL = IN[3]\r\n\r\n# END: DEFINE INPUTS\r\n\r\n# START: DEFINE CLASSES\r\nclass HelloWorldForm(Form):\r\n\r\n    def __init__(self):\r\n        self.Text = 'Hello World'\r\n        self.Name = 'Hello World'\r\n        self.Height = 1000\r\n        self.Width = 800\r\n        \r\n        self.image = PictureBox()\r\n        self.image.Location = Point(40, 40)\r\n        self.image.Size = Size(700, 700)\r\n        self.image.SizeMode = PictureBoxSizeMode.Zoom\r\n        self.image.Load(imageURL)\r\n        self.Controls.Add(self.image)\r\n        \r\n    \tself.label = Label()\r\n    \tself.label.Text = message\r\n    \tself.label.Font = Font(\"Arial\", 12,FontStyle.Bold)\r\n        self.label.Location = Point(40, 850)\r\n        self.label.MaximumSize = Size(700, 0)\r\n        self.label.AutoSize = 1\r\n        self.Controls.Add(self.label)\r\n          \r\n        self.button = Button()\r\n    \tself.button.Text = \"Click button to view project details\"\r\n        self.button.DialogResult = DialogResult.OK\r\n    \tself.button.Font = Font(\"Arial\", 11,FontStyle.Regular)\r\n        self.button.Location = Point(40, 750)\r\n        self.button.Height = 40\r\n        self.button.Width = 700\r\n        self.button.Click += self.buttonPressed\r\n        self.Controls.Add(self.button)\r\n\r\n        \r\n    def buttonPressed(self, sender, args):\r\n\t\turl = portfolioItemURL\r\n\t\thtml = webbrowser.open(url, new=2)\r\n\t\tself.Close()\r\n\r\nform = HelloWorldForm()\r\n# END: DEFINE CLASSES\r\n\r\n# START: TEST FOR INPUT BY USER\r\nif boolean == 1:\r\n   Application.Run(form);\r\n   text = \"Click the button to see something interesting and close the form!\";\r\nelse:\r\n   text = \"Waiting for user to say yes!\";\r\n# END: TEST FOR INPUT BY USER\r\n\r\nOUT = text\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n",
